package my;

abstract class Animal1 {
	abstract void cats();
	abstract void dogs();
}
	
	class Cats extends Animal1 {
		
		@Override
		void cats() {
			System.out.println("Cats meow..");
				
		}
		@Override
		void dogs() {
			System.out.println();
		}	
	}
	class Dogs extends Animal1 {
		@Override
		void cats() {
			System.out.println();
		}
		@Override
		void dogs() {
			System.out.println("Dog barks..");
			
		}	
	
		public class Animal{
	public static void main(String[] args) {
	
		Cats cat = new Cats();
		Dogs dog = new Dogs();
		
		dog.dogs();
		cat.cats();
	}

	}

}
