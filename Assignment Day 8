Ques 1.Painting Shapes
Ans. 1
package myPackage;

import java.util.Scanner;

abstract class Shape{
	String shape;
	abstract void area();
	
	
	public String toString() {
		return "Shape: " + shape + "]";
	}
	
}
class Sphere extends Shape{
	String shapeName;
	
	@Override
	void area() {
		System.out.println("Enter radius of Sphere: ");
		double radius = new Scanner(System.in).nextInt();
		
		System.out.println("Paint needed to paint this Sphere: ");
		System.out.println(4*(3.14)*(radius*radius));
	}
	
}
class Rectangle extends Shape{
	
	String shapeName;
	
	@Override
	void area() {
		System.out.println("Enter height of Rectangle: ");
		int a = new Scanner(System.in).nextInt();
		System.out.println("Enter breadth of Rectangle: ");
		int b = new Scanner(System.in).nextInt();
		
		System.out.println("Paint needed to paint this Rectangle: ");
		System.out.println(a*b);
		
	}
}
class Cylinder extends Shape{
	String shapeName;
	
	@Override
	void area() {
		System.out.println("Enter radius of cylinder:- ");
		double radius = new Scanner(System.in).nextDouble();
		System.out.println("Enter height of cylinder:- ");
		double h = new Scanner(System.in).nextDouble();
		
		System.out.println("Paint needed to paint this cylinder: ");
		System.out.println(2*3.14*radius*(radius+h));
	
	}
}


public class sixFeb {
	public static void main(String args[]) {
		System.out.println("Shape: Sphere");
		
		Sphere sp = new Sphere();
		sp.shapeName = "Sphere";
		sp.area();
		
		System.out.println();
		System.out.println("Shape: Retangle");
		Rectangle rt = new Rectangle();
		rt.shapeName = "Rectangle";
		rt.area();
		
		System.out.println();
		System.out.println("Shape: Cylinder");
		Cylinder cy = new Cylinder();
		cy.shapeName = "Cylinder";
		cy.area();
	}

}
-------------------------
